{
  "compilerOptions": {
    "incremental": true,
    "skipLibCheck": true,
    "downlevelIteration": true,
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmitOnError": true
  }
}
